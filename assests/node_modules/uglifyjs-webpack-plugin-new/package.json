{
  "name": "uglifyjs-webpack-plugin-new",
  "version": "1.0.0",
  "description": "UglifyJS plugin for webpack",
  "license": "MIT",
  "repository": "webpack-contrib/uglifyjs-webpack-plugin",
  "author": "webpack Contrib Team",
  "homepage": "https://github.com/webpack-contrib/uglifyjs-webpack-plugin",
  "bugs": "https://github.com/webpack-contrib/uglifyjs-webpack-plugin/issues",
  "main": "dist/cjs.js",
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "start": "npm run build -- -w",
    "commitlint": "commitlint --from=master",
    "lint:prettier": "prettier \"{**/*,*}.{js,json,md,yml,css}\" --list-different",
    "lint:js": "eslint --cache src test",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "release": "standard-version",
    "security": "npm audit",
    "test:only": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --collectCoverageFrom=\"src/**/*.js\" --coverage",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test npm run test:coverage",
    "defaults": "webpack-defaults"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "webpack": "^5.89.0"
  },
  "dependencies": {
    "cacache": "^18.0.1",
    "find-cache-dir": "^5.0.0",
    "is-wsl": "^3.1.0",
    "schema-utils": "^4.2.0",
    "serialize-javascript": "^6.0.1",
    "source-map": "^0.7.4",
    "uglify-js": "^3.17.4",
    "webpack-sources": "^3.2.3",
    "worker-farm": "^1.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@webpack-contrib/defaults": "^6.3.0",
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "babel-jest": "^29.7.0",
    "commitlint-azure-pipelines-cli": "^1.0.5",
    "cross-env": "^7.0.3",
    "del": "^7.1.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.0",
    "memory-fs": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "standard-version": "^9.5.0",
    "terser": "^5.26.0",
    "webpack": "^5.89.0"
  },
  "keywords": [
    "uglify",
    "uglify-js",
    "uglify-es",
    "webpack",
    "webpack-plugin"
  ]
}
